@model List<Tomasos.Models.Identity.Matratt>

<div class="text-center">
    <h3>Your cart</h3>
</div>
<div class="my-2">
    @foreach (var dish in Model)
    {
        @dish.MatrattNamn
        <br/>
    }
</div>
<div class="cart-total my-2">
    Total:
    <div class="float-right">@Model.Sum(e => e.Pris) kr</div>
</div>
<div class="text-center">
    <button type="button" class="btn btn-lg btn-primary btn-block">Buy</button>
</div>



